<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="app">
        <h1>You search: <%= data.userQuery %></h1>
        <% if(data.loggedIn){ %>
            <h2>Welcome back, <%= data.username %></h2>
        <% }%>
        <ul>
            <% data.searchResults.forEach(result=>{ %>
                <li><%= result %></li>
            <% }); %>
        </ul>
    </div>
    <script src="/public/js/example.js"></script>
</body>
</html>